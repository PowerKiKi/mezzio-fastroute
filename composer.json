{
    "name": "mezzio/mezzio-fastroute",
    "description": "FastRoute integration for Mezzio",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mezzio",
        "FastRoute",
        "http",
        "middleware",
        "psr",
        "psr-7"
    ],
    "homepage": "https://mezzio.dev",
    "support": {
        "docs": "https://docs.mezzio.dev/mezzio-fastroute/",
        "issues": "https://github.com/mezzio/mezzio-fastroute/issues",
        "source": "https://github.com/mezzio/mezzio-fastroute",
        "rss": "https://github.com/mezzio/mezzio-fastroute/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "fig/http-message-util": "^1.1",
        "laminas/laminas-stdlib": "^3.1",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "mezzio/mezzio-router": "^2.0",
        "nikic/fast-route": "^1.0.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "~1.0.0",
        "malukenho/docheader": "^0.1.5",
        "phpunit/phpunit": "^4.7 || ^5.6"
    },
    "autoload": {
        "psr-4": {
            "Mezzio\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MezzioTest\\Router\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@license-check",
            "@test"
        ],
        "coveralls": "coveralls",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "license-check": "docheader check src/ test/",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    },
    "replace": {
        "zendframework/zend-expressive-fastroute": "self.version"
    }
}
